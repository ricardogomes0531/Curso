@model List<I9Solucoes.Models.CanalYoutube>
<section>
	<div class="row">
		<div class="col-lg-3">
			<h1>Vídeos do Canal Visão de DEV no Youtube</h1>
		</div>
		<div class="col-lg-8">
			@if (Model.Count() > 0)
			{
				foreach (var canal in Model.ToList())
				{
<h2><a href="@canal.Link" target="_blank">@canal.Titulo</a></h2>					
			<p class="text-lead">@canal.Descricao</p>
							}
			}
			else
			{
				<p class="text-lead">Nenhum vídeo para mostrar</p>
			}
		</div>
	</div>
</section>
<hr>
